.info{
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  background-color: white;
  margin-bottom: 20rpx;
  align-items: center;
}
.info-avatarUrl{
  width: 150rpx;
  height: 150rpx;

}
.info-avatarUrl image{
  width: 100%;
  height: 100%;
}
.info-name{
  width: 100rpx;
  height: 50rpx;

}
.address{
  width: 100%;
  height: 100rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  align-items: center;

}
.address-icon{
  width: 50rpx;
  height: 50rpx;
  margin-left: 20rpx;
  margin-right: 20rpx; 
}
.address-icon image{
  width: 100%;
  height: 100%;
}

.address-text{
  width: 200rpx;
  height: 100rpx;
  line-height: 100rpx;

}
