.my{
  width: 100%;
  height: 320rpx;
  display: flex;
  flex-direction: column;
  border-radius: 20px;
  background-color: white;
  align-items: center;
}
.my-title{
  width: 90%;
  height: 100rpx;
  border-bottom: 1px solid #f8fbf8;
  font-weight: bold;
  font-size: 40rpx;
  display: table-cell;
  text-align: center;
  line-height: 100rpx;
}
.my-list{
  width: 100%;
  height: 300rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;

}
.my-list-cell{
  width: 150rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
}
.my-list-cell-icon{
  width: 150rpx;
  height: 150rpx;
  
}
.my-list-cell-icon image{
  width: 100%;
  height: 100%
}

.my-list-cell-text{
  width: 150rpx;
  height: 50rpx;
  display: table-cell;
  text-align: center;
  line-height: 50rpx;
}
